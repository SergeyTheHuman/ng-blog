/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
			"@environments/*": ["./src/environments/*"],
			"@admin/*": ["./src/app/admin/*"],
			"@main/*": ["./src/app/main/*"],
			"@shared/*": ["./src/app/shared/*"]
		}
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
